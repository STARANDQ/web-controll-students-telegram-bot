<!DOCTYPE html>
<html lang="en">
<head>
  <title>Table V02</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!--===============================================================================================-->
  <link rel="icon" type="image/png" href="/administrator/listCurators/images/icons/favicon.ico"/>
  <!--===============================================================================================-->
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/vendor/bootstrap/css/bootstrap.min.css">
  <!--===============================================================================================-->
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
  <!--===============================================================================================-->
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/vendor/animate/animate.css">
  <!--===============================================================================================-->
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/vendor/select2/select2.min.css">
  <!--===============================================================================================-->
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/vendor/perfect-scrollbar/perfect-scrollbar.css">
  <!--===============================================================================================-->
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/css/util.css">
  <link rel="stylesheet" type="text/css" href="/administrator/listCurators/css/main.css">
  <!--===============================================================================================-->
</head>
<body>

    <div class="wrap-table100">
      <div class="table">

        <div class="row header">
          <div class="cell">
            Повне Ім'я
          </div>
          <div class="cell">
            Email
          </div>
          <div class="cell">
            Номер телефону
          </div>
          <div class="cell">
            Навчальний заклад
          </div>
        </div>


        <% arrCurator.forEach(curator => { %>
          <div class="row">
            <div class="cell" data-title="Full Name">
              <%= curator.name %>
            </div>
            <div class="cell" data-title="Age">
              <%= curator.email %>
            </div>
            <div class="cell" data-title="Job Title">
              <%= curator.phoneNumber %>
            </div>
            <div class="cell" data-title="Location">
              <%= curator.educational_Institution %>
            </div>
          </div>
        <% })%>

      </div>
    </div>



<!--===============================================================================================-->
<script src="../../../../../views/administrator/listCurators/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
<script src="../../../../../views/administrator/listCurators/vendor/bootstrap/js/popper.js"></script>
<script src="../../../../../views/administrator/listCurators/vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
<script src="../../../../../views/administrator/listCurators/vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
<script src="../../../../../views/administrator/listCurators/js/main.js"></script>

</body>
</html>